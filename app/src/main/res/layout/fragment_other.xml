    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/X27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.58" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/X28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.61" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/X29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.625" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/X30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.645" />

    <Button
        android:id="@+id/sale_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/another_button"
        android:stateListAnimator="@null"
        android:clickable="true"
        android:elevation="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/X2"
        app:layout_constraintLeft_toLeftOf="@id/Y3"
        app:layout_constraintRight_toRightOf="@id/Y4"
        app:layout_constraintTop_toTopOf="@+id/X1" />

    <Button
        android:id="@+id/promo_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/another_button"
        android:stateListAnimator="@null"
        android:clickable="true"
        android:elevation="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/X4"
        app:layout_constraintLeft_toLeftOf="@id/Y3"
        app:layout_constraintRight_toRightOf="@id/Y4"
        app:layout_constraintTop_toTopOf="@+id/X3" />

    <Button
        android:id="@+id/fact_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/another_button"
        android:stateListAnimator="@null"
        android:clickable="true"
        android:elevation="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/X6"
        app:layout_constraintLeft_toLeftOf="@id/Y3"
        app:layout_constraintRight_toRightOf="@id/Y4"
        app:layout_constraintTop_toTopOf="@+id/X5" />

    <Button
        android:id="@+id/notes_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/another_button"
        android:stateListAnimator="@null"
        android:clickable="true"
        android:elevation="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/X30"
        app:layout_constraintLeft_toLeftOf="@id/Y3"
        app:layout_constraintRight_toRightOf="@id/Y4"
        app:layout_constraintTop_toTopOf="@+id/X25" />

    <ImageView
        android:id="@+id/forward1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/next"
        app:layout_constraintBottom_toBottomOf="@id/X11"
        app:layout_constraintLeft_toLeftOf="@id/Y5"
        app:layout_constraintRight_toRightOf="@id/Y6"
        app:layout_constraintTop_toTopOf="@id/X9"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/forward2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/next"
        app:layout_constraintBottom_toBottomOf="@id/X13"
        app:layout_constraintLeft_toLeftOf="@id/Y5"
        app:layout_constraintRight_toRightOf="@id/Y6"
        app:layout_constraintTop_toTopOf="@id/X12"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/forward3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/next"
        app:layout_constraintBottom_toBottomOf="@id/X15"
        app:layout_constraintLeft_toLeftOf="@id/Y5"
        app:layout_constraintRight_toRightOf="@id/Y6"
        app:layout_constraintTop_toTopOf="@id/X14"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/forward5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/next"
        app:layout_constraintBottom_toBottomOf="@id/X28"
        app:layout_constraintLeft_toLeftOf="@id/Y5"
        app:layout_constraintRight_toRightOf="@id/Y6"
        app:layout_constraintTop_toTopOf="@id/X27"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/sale"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/discount"
        app:layout_constraintBottom_toBottomOf="@id/X19"
        app:layout_constraintLeft_toLeftOf="@id/Y7"
        app:layout_constraintRight_toRightOf="@id/Y8"
        app:layout_constraintTop_toTopOf="@id/X18"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/promo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/promo"
        app:layout_constraintBottom_toBottomOf="@id/X21"
        app:layout_constraintLeft_toLeftOf="@id/Y7"
        app:layout_constraintRight_toRightOf="@id/Y8"
        app:layout_constraintTop_toTopOf="@id/X20"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/fact"
        android:layout_width="45dp"
        android:layout_height="47dp"
        android:elevation="2dp"
        android:src="@drawable/fact"
        app:layout_constraintBottom_toBottomOf="@id/X23"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@id/Y7"
        app:layout_constraintRight_toRightOf="@id/Y8"
        app:layout_constraintTop_toTopOf="@id/X22"
        app:layout_constraintVertical_bias="0.0"
        app:tint="#FFFFFF" />

    <ImageView
        android:id="@+id/notes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:src="@drawable/notes"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintBottom_toBottomOf="@id/X29"
        app:layout_constraintLeft_toLeftOf="@id/Y7"
        app:layout_constraintRight_toRightOf="@id/Y8"
        app:layout_constraintTop_toTopOf="@id/X26"
        app:tint="#FFFFFF" />

    <TextView
        android:id="@+id/sale_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/sintony"
        android:gravity="left|center_vertical"
        android:text="Current Sales"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/X19"
        app:layout_constraintLeft_toLeftOf="@id/Y9"
        app:layout_constraintRight_toRightOf="@id/Y5"
        app:layout_constraintTop_toTopOf="@id/X18" />

    <TextView
        android:id="@+id/promo_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/sintony"
        android:gravity="left|center_vertical"
        android:text="Coupons"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/X21"
        app:layout_constraintLeft_toLeftOf="@id/Y9"
        app:layout_constraintRight_toRightOf="@id/Y5"
        app:layout_constraintTop_toTopOf="@id/X20" />

    <TextView
        android:id="@+id/fact_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/sintony"
        android:gravity="left|center_vertical"
        android:text="Random Money Facts"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/X23"
        app:layout_constraintLeft_toLeftOf="@id/Y9"
        app:layout_constraintRight_toRightOf="@id/Y5"
        app:layout_constraintTop_toTopOf="@id/X22" />

    <TextView
        android:id="@+id/notes_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/sintony"
        android:gravity="left|center_vertical"
        android:text="Notes"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/X29"
        app:layout_constraintLeft_toLeftOf="@id/Y9"
        app:layout_constraintRight_toRightOf="@id/Y5"
        app:layout_constraintTop_toTopOf="@id/X26" />

