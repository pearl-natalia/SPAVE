        android:id="@+id/dateView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="5dp"
        android:fontFamily="@font/sintony"
        android:gravity="center"
        android:textAllCaps="false"
        android:textColor="#BDBFBE"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@id/X1"
        app:layout_constraintLeft_toLeftOf="@id/Y7"
        app:layout_constraintRight_toRightOf="@id/Y6"
        app:layout_constraintTop_toTopOf="@id/X10" />



